# PetSignal - Environment Variables Example
# Copy this file to .env and fill in your values
# IMPORTANT: Never commit real credentials to the repository

# API Configuration
EXPO_PUBLIC_API_BASE_URL=http://your-api-server:8080

# Admin Credentials for User Data Access
# SECURITY: These must be set in environment variables
# NEVER hardcode these values in source code
EXPO_PUBLIC_ADMIN_USERNAME=your_admin_username_here
EXPO_PUBLIC_ADMIN_PASSWORD=your_admin_password_here

# Google Places API Configuration
EXPO_PUBLIC_GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Legacy Configuration (for reference)
API_TIMEOUT=10000
DEV_API_IP=192.168.1.100
DEV_API_PORT=8080
PROD_API_URL=https://your-production-server.com
DEBUG_MODE=true
ENABLE_NETWORK_LOGS=true
